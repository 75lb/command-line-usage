{
  "name": "command-line-usage",
  "author": "Lloyd Brookes <75pound@gmail.com>",
  "version": "2.0.2",
  "description": "Generates command-line usage information",
  "repository": "https://github.com/75lb/command-line-usage.git",
  "license": "MIT",
  "main": "index",
  "keywords": [
    "terminal",
    "command line",
    "usage",
    "generator"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "docs": "jsdoc2md -t jsdoc2md/README.hbs --no-gfm lib/*.js > README.md; echo",
    "test": "tape test/*.js",
    "es5": "babel --no-comments lib --out-dir es5"
  },
  "dependencies": {
    "ansi-escape-sequences": "^2.2.0",
    "array-back": "^1.0.2",
    "column-layout": "^2.1",
    "feature-detect-es6": "^1.2.0",
    "object-tools": "^2",
    "typical": "^2.4.2"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^1.3.3",
    "tape": "^4.4.0"
  },
  "standard": {
    "ignore": [
      "es5"
    ]
  }
}
